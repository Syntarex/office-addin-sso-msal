        "start": "HOST=0.0.0.0 PORT=8080 node ./dist/server/entry.mjs"
    },
    "dependencies": {
        "@astrojs/check": "^0.9.5",
        "@astrojs/node": "^9.5.1",
        "@azure/msal-browser": "^3.28.0",
        "@libsql/client": "^0.15.15",
        "@oslojs/crypto": "^1.0.1",
        "@oslojs/encoding": "^1.1.0",
        "@pnp/core": "^4.17.0",
        "@pnp/graph": "^4.17.0",
        "@pnp/nodejs": "^4.17.0",
        "@pnp/queryable": "^4.17.0",
        "@vitejs/plugin-basic-ssl": "^2.1.0",
        "arctic": "^3.7.0",
        "astro": "^5.15.9",
        "chalk": "^5.6.2",
        "drizzle-kit": "^0.31.7",
        "drizzle-orm": "^0.44.7",
        "radash": "^12.1.1",
        "sharp": "^0.34.5",
        "zod": "^3.25.76"
