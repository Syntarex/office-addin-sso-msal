{
    "name": "saxware-example",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "astro": "astro",
        "prebuild": "drizzle-kit generate --config ./db.config.js",
        "build": "astro build",
        "clean": "rimraf ./node_modules/.astro && rimraf ./.astro && rimraf ./dist && rimraf ./deploy.zip",
        "dev": "astro dev --port 3000",
        "migrate": "drizzle-kit generate --config ./db.config.js && drizzle-kit migrate --config ./db.config.js",
        "start": "HOST=0.0.0.0 PORT=8080 node ./dist/server/entry.mjs"
    },
    "dependencies": {
        "@astrojs/node": "^9.5.1",
        "@azure/msal-browser": "^3.28.0",
        "@libsql/client": "^0.15.15",
        "@oslojs/crypto": "^1.0.1",
        "@oslojs/encoding": "^1.1.0",
        "@pnp/core": "^4.17.0",
        "@pnp/graph": "^4.17.0",
        "@pnp/nodejs": "^4.17.0",
        "@pnp/queryable": "^4.17.0",
        "@vitejs/plugin-basic-ssl": "^2.1.0",
        "arctic": "^3.7.0",
        "astro": "^5.15.9",
        "drizzle-kit": "^0.31.7",
        "drizzle-orm": "^0.44.7"
    },
    "devDependencies": {
        "rimraf": "^6.1.0"
    },
    "engines": {
        "node": ">=22",
        "npm": ">=10"
    }
}
